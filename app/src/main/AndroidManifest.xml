<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ap1307.stockapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetInfo"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.ap1307.stockapp.GetInfo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
>
        <activity
            android:name=".HistoricalQuotes"
            android:label="@string/title_activity_historical_quotes" >
            <intent-filter>
                <action android:name="com.example.ap1307.stockapp.HistoricalQuotes" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LiveStockActivity"
            android:label="@string/title_activity_live_stock" >
        </activity>
        <activity
            android:name=".Help"
            android:label="@string/title_activity_help" >
        </activity>
    </application>

</manifest>
